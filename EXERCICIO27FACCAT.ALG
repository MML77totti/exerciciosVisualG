algoritmo "Exercicio27Faccat"
var
   valor:inteiro
inicio
   Escreval("Digite um valor:")
   Leia(valor)

   se valor<0 entao
      Escreval("O número é negativo")
   senao
      se valor = 0 entao
         Escreval("O número digitado é Zero")
      senao
         Escreval("O número é positivo")
      fimse
   fimse
fimalgoritmo