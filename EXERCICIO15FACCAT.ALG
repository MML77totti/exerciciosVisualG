algoritmo "Exercicio15Faccat"
var
   numero1 : inteiro
inicio
      Escreval ("Digite um Numero: ")
      Leia (numero1)
      
      se numero1 < 0 entao
         Escreval ("O numero digitado foi ", numero1, " é um numero NEGATIVO")
      senao
           Escreval ("O numero digitado foi ", numero1, " é um numero POSITIVO")
      fimse
fimalgoritmo